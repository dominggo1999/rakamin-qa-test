{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["es2015", "dom"],
    "types": [
      "cypress",
      "node",
      "cypress-real-events",
      "@testing-library/cypress"
    ],
    "typeRoots": ["./cypress/support/commands.ts"],
    "baseUrl": "cypress",
    "paths": {
      "config/*": ["./cypress/config/*"],
      "page-objects/*": ["./cypress/page-objects/*"],
      "section-objects/*": ["./cypress/section-objects/*"]
    }
  },
  "include": ["**/*.ts", "**/*.cy.ts", "**/*.spec.ts", "webpack.config.js"]
}
